# Maven target directory (where compiled files and artifacts are stored)
target/

# Eclipse project files
.classpath
.project
.settings/

# IntelliJ IDEA project files
.idea/
*.iml

# VSCode project files
.vscode/

# Logs
*.log

# Dependency directory for maven (not needed to track)
*.jar
*.war
*.ear

# Files generated by build tools (for example, the local Maven repository)
.mvn/
.mvn/*

# JUnit output (if using the JUnit testing framework)
surefire-reports/

# OS generated files
.DS_Store
Thumbs.db

# Maven wrapper files
.mvn/wrapper/maven-wrapper.jar
.mvn/wrapper/maven-wrapper.properties

# IntelliJ project output directories (when using IntelliJ IDEA)
out/

# Java class files
*.class

# Ignore files for packaging (e.g. if creating a jar/war file locally)
*.zip
*.tar.gz
*.tgz

# JetBrains IDE (PhpStorm, RubyMine, etc.)
.idea/

# Mac system files
.DS_Store

# Windows system files
Thumbs.db
